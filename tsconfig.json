{
  "compilerOptions": {
    "sourceMap": true,
    "strict": true,
    "module": "ESNext",
    "target": "ESNext",
    "newLine": "lf",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "moduleResolution": "node",
    "noEmitOnError": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "jsxFactory": "jsx"
  }
}
